    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch owl2proto with example",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/owl2proto",
            "cwd": "${workspaceFolder}",
            "args": [
                "generate-proto",
                "--root-resource-name=ex:Resource",
                "example/cloud.owx",
                "--header-file=example/example_header.proto",
                "--output-path=example/example.proto"
            ]
        }
    ]
}